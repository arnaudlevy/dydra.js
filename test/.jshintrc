{
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "undef": true,
  "unused": "vars",
  "trailing": true,
  "maxlen": 100,
  "devel": true,
  "predef": [
    "QUnit",
    "asyncTest",
    "deepEqual",
    "equal",
    "expect",
    "module",
    "notDeepEqual",
    "notEqual",
    "notPropEqual",
    "notStrictEqual",
    "ok",
    "propEqual",
    "raises",
    "start",
    "stop",
    "strictEqual",
    "test",
    "throws"
  ]
}
